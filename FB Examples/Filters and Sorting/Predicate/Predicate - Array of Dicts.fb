/*  Predicate - Array of Dicts  Bernie Wylde 20180704*/include "NSLog.incl"void local fn DoItCFMutableArrayRef array = fn MutableArrayWithCapacity(0)CFDictionaryRef dict = @{@"FirstName":@"Ken",@"LastName":@"Shmidheiser"}MutableArrayAddObject( array, dict )dict = @{@"FirstName":@"Steve",@"LastName":@"Van Voorst"}MutableArrayAddObject( array, dict )dict = @{@"FirstName":@"Brian",@"LastName":@"Stevens"}MutableArrayAddObject( array, dict )dict = @{@"FirstName":@"Eugen",@"LastName":@"Rosu"}MutableArrayAddObject( array, dict )dict = @{@"FirstName":@"Brian",@"LastName":@"Garman"}MutableArrayAddObject( array, dict )// equalsPredicateRef pred = fn PredicateWithFormat( @"(FirstName == %@)", @"Brian" )CFArrayRef result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"== FirstName equals 'Brian' ==\n%@\n", result)// begins withpred = fn PredicateWithFormat( @"(%K beginswith[cd] %@)", @"FirstName", @"K" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"== FirstName begins with 'K' ==\n%@\n", result)// containspred = fn PredicateWithFormat( @"(LastName contains %@)", @"s" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"== LastName contains 's' ==\n%@\n", result)// ends withpred = fn PredicateWithFormat( @"(%K endswith[cd] %@)", @"LastName", @"man" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"== LastName ends with 'man' ==\n%@\n", result)end fnfn DoItHandleEvents