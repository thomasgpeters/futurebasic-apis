/*  Predicate - Array of Strings  Bernie Wylde 20180704*/include "NSLog.incl"void local fn DoItCFArrayRef array = @[@"Ken Shmidheiser",@"Michele Neri",@"Steve Van Voorst",@"Brian Stevens",@"Alain Pastor",@"Eugen Rosu",@"Brian Garman",@"Rich Love"]// equalsPredicateRef pred = fn PredicateWithFormat( @"(SELF == %@)", @"Eugen Rosu" )CFArrayRef result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"Equals 'Eugen Rosu'\n%@\n", result)// containspred = fn PredicateWithFormat( @"(SELF contains %@)", @"e" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"Contains 'e'\n%@\n", result)// containspred = fn PredicateWithFormat( @"(SELF contains %@)", @"S" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"Contains 'S'\n%@\n", result)// containspred = fn PredicateWithFormat( @"(SELF contains %@)", @"s" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"Contains 's'\n%@\n", result)// containspred = fn PredicateWithFormat( @"(SELF contains[c] %@)", @"S" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"Contains 'S' or 's'\n%@\n", result)// beginswithpred = fn PredicateWithFormat( @"(SELF beginswith %@)", @"Brian" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"Begins with 'Brian'\n%@\n", result)// endswithpred = fn PredicateWithFormat( @"(SELF endswith %@)", @"man" )result = fn ArrayFilteredArrayUsingPredicate( array, pred )NSLog(@"Ends with 'man'\n%@", result)end fnfn DoItHandleEvents