/*  va_list demo 3*/include "NSLog.incl"local fn Average( numArgs as long, ... ) as doubleva_list   aplong      i, valueva_start( ap, numArgs )double sum = 0.0for i = 1 to numArgsvalue = fn va_arglong(ap)NSLog(@"arg%ld:\t%ld",i,value)sum += valuenext iNSLog(@"----------------")NSLog(@"sum:\t\t%0.0f",sum)va_end( ap )end fn = sum / numArgsvoid local fn DoItdouble avg = fn Average( 3, 14, 21, 102 )NSLog(@"----------------")NSLog(@"avg:\t\t%0.2f",avg)end fnfn DoItHandleEvents