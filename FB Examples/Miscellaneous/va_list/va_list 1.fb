/*   va_list demo 1*/include "NSLog.incl"void local fn MyFunction( count as long, ... )va_list   aplong      i, value/*  initialise the ap variable.  count is the last known fixed argument passed to the function, i.e. the arg before the ellipsis.*/va_start( ap, count )for i = 1 to countvalue = fn va_arglong( ap )// retrieve the next 'long' argument in listNSLog( @"arg%d:\t\t%d",i,value )// do something with valuenext i// clean up (must be called before returning from function)va_end( ap )end fnfn MyFunction( 3, 123, 456, 789 )HandleEvents