/*  MenuItemActionCallback1  one menu item*/include "NSLog.incl"void local fn MyMenuItemCallback( menuID as NSInteger, itemID as NSInteger )NSLog(@"action callback - menuID: %ld, itemID: %ld",menuID,itemID)end fnvoid local fn BuildMenumenu 1,,1,@"File"menu 1,0,1,@"New…",@"n"// params: menuID, itemID, callback fn, userDataMenuItemSetActionCallback( 1, 0, @fn MyMenuItemCallback, NULL )menu 1,1,1,@"Open…",@"o"end fnvoid local fn BuildWindowwindow 1, @"Choose a menu item", (0,0,300,100), NSWindowStyleMaskTitledend fnvoid local fn DoMenu( menuID as long, itemID as long )NSLog(@"on menu fn - menuID: %ld, itemID: %ld",menuID,itemID)end fnfn BuildMenufn BuildWindowon menu fn DoMenuHandleEvents