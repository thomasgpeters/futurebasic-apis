/*  AppSetTimer  Bernie Wylde 20171017*/local fn Timer1FiredDateFormatterRef df = fn DateFormatterInitDateFormatterSetDateFormat( df, @"hh:mm:ss:SSS" )CFStringRef string = fn DateFormatterStringFromDate( df, fn DateInit )textlabel 1, string,, 1end fnlocal fn Timer2FiredDateFormatterRef df = fn DateFormatterInitDateFormatterSetDateFormat( df, @"hh:mm:ss" )CFStringRef string = fn DateFormatterStringFromDate( df, fn DateInit )textlabel 2, string,, 1end fnlocal fn Timer3Firedtextlabel 3, @"Timer 3 fired",, 1end fnvoid local fn BuildWindowwindow 1, @"Timer", (0,0,480,270), NSWindowStyleMaskTitledtextlabel 1,, (195,196,90,17)textlabel 2,, (195,160,90,17)textlabel 3, @"< will fire in 5 seconds >", (195,124,160,17)textlabel 4, @"Timer 1:", (120,196,90,17)textlabel 5, @"Timer 2:", (120,160,90,17)textlabel 6, @"Timer 3:", (120,124,90,17)end fnfn BuildWindowfn AppSetTimer( 1.0/60, @fn Timer1Fired, YES )fn AppSetTimer( 1.0, @fn Timer2Fired, YES )fn AppSetTimer( 5.0, @fn Timer3Fired, NO )HandleEvents