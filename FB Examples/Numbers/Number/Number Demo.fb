/*  Number Demo*/include "NSLog.incl"void local fn DoItCFNumberRef     b, c, s, l, llCFNumberRef     uc, us, ul, ullCFNumberRef     f, dBoolean         boolchar            chrshort           shtlong            lngSInt64           lnglngunsigned char    unschrunsigned short   unsshtunsigned long    unslngUInt64           unslnglngfloat            fltdouble           dblb = fn NumberWithBool( YES )             // shorthand: @(YES)c = fn NumberWithChar( _"A" )            // shorthand: @(_"A")s = fn NumberWithShort( 32767 )          // shorthand: @32767l = fn NumberWithLong( 2147483647 )      // shorthand: @2147483647ll = fn NumberWithLongLong( 4294967295 ) // shorthand: @4294967295NSLog(@"bool: %@\rchar: %@\rshort: %@\rlong: %@\rlong long: %@",b,c,s,l,ll)uc = fn NumberWithUnsignedChar( _"Z" )us = fn NumberWithUnsignedShort( _"A" )ul = fn NumberWithUnsignedLong( 65535 )ull = fn NumberWithUnsignedLongLong( 4294967295 )f = fn NumberWithFloat( 1.234 )d = fn NumberWithDouble( 9876.54321 )NSLog(@"unsigned char: %@\runsigned short: %@\runsigned long: %@\rfloat: %@\rdouble: %@\r\r",uc,us,ul,f,d)bool = fn NumberBoolValue( b )chr = fn NumberCharValue( c )sht = fn NumberShortValue( s )lng = fn NumberLongValue( l )lnglng = fn NumberLongLongValue( ll )NSLog(@"%d, %d, %d, %d, %u",bool,chr,sht,lng,lnglng)unschr = fn NumberUnsignedCharValue( uc )unssht = fn NumberUnsignedShortValue( us )unslng = fn NumberUnsignedLongValue( ul )unslnglng = fn NumberUnsignedLongLongValue( ull)flt = fn NumberFloatValue( f )dbl = fn NumberDoubleValue( d )NSLog(@"%c, %d, %d, %qu, %0.3f, %0.5f",unschr,unssht,unslng,unslnglng,flt,dbl)end fnfn DoItHandleEvents