include "NSLog.incl"long i// \b is similar to using a semi-colon in FB's old PRINT ( to a console window ) statement.// Effectively it backspace deletes over the the carriage return that would otherwise be there.// NSLog uses C's method of escaping ( \ ) to achieve its result.NSLog(@"An excellent cribbage hand is: ")NSLog(@"5♣️,\b")NSLog(@"5♦️,\b")NSLog(@"5♥️,\b")NSLog(@"5♠️\b")NSLog(@" with a cut: J♣️")// show tabbingNSLog(@"\nHere's some tabbing")for i = 1 to 9NSLog(@"%ld\t",i)nextNSLog(@"")HandleEvents