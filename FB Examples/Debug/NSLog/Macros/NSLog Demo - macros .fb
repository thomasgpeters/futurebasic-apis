/*  NSLog Demo - macros  Debug macros (from handy.incl header)    FUNCTION__    PRETTY_FUNCTION__    LINE__    FILE__    DATE__    TIME__*/include "handy.incl"// above macros defined in this headerinclude "NSLog.incl"void local fn BuildWindowwindow 1, @"NSLog Demo", (0,0,550,400)button 1,,, @"Button 1", (14,352,92,32)button 2,,, @"Button 2", (14,319,92,32)NSLog(@"fn %s\n",FUNCTION__)end fnvoid local fn Button1ActionNSLog(@"fn %s\n",FUNCTION__)end fnvoid local fn Button2ActionNSLog(@"fn %s",FUNCTION__)NSLog(@"file: %s",FILE__)NSLog(@"line: %d\n",LINE__)end fnvoid local fn DoDialog( ev as long, tag as long )select ( ev )case _btnClickselect ( tag )case 1 : fn Button1Actioncase 2 : fn Button2Actionend selectend selectend fnNSLog(@"Welcome to NSLog v%@\n",NSLogVers)fn BuildWindowon dialog fn DoDialogHandleEvents