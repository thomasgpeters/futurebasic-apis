void local fn BuildWindowwindow 1, @"Window 1", (0,600,300,150), NSWindowStyleMaskTitledbutton 1,,, @"Go to MyAnchor", (14,13,130,32)end fnvoid local fn DoDialog( ev as long )CFStringRef   bookNameselect ( ev )case _btnClickbookName = fn BundleObjectForInfoDictionaryKey( fn BundleMain, @"CFBundleHelpBookName" )fn HelpManagerOpenHelpAnchor( @"MyAnchor", bookName )end selectend fnhelpmenu 1fn BuildWindowon dialog fn DoDialogHandleEvents