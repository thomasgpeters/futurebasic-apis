beginCDeclaration#import <Carbon/Carbon.h>endCinclude "Util_AppleHelp.incl"void local fn BuildWindowswindow 1, @"Window 1", (0,600,300,150), NSWindowStyleMaskTitledbutton 1,,, @"", (18,17,25,25),, NSBezelStyleHelpButtonViewSetToolTip( 1, @"Goto Window 1 Help" )window 2, @"Window 2", (360,600,300,150), NSWindowStyleMaskTitledbutton 1,,, @"", (18,17,25,25),, NSBezelStyleHelpButtonViewSetToolTip( 1, @"Goto Window 2 Help" )window 3, @"Window 3", (180,390,300,150), NSWindowStyleMaskTitledbutton 1,,, @"", (18,17,25,25),, NSBezelStyleHelpButtonViewSetToolTip( 1, @"Goto Window 3 Help" )end fnvoid local fn DoDialog( ev as long, tag as long, wnd as long )select ( ev )case _btnClickselect ( wnd )case 1 : fn HBGotoPage( @"Pages/page1.html", NULL )case 2 : fn HBGotoPage( @"Pages/page2.html", NULL )case 3 : fn HBGotoPage( @"Pages/page3.html", NULL )end selectend selectend fnhelpmenu 1fn BuildWindowson dialog fn DoDialogHandleEvents