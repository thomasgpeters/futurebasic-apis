_window = 1begin enum 1_label_txtFld_remFocusBtn_checkbox_pushBtnend enumvoid local fn BuildWindowwindow _window, @"ResetCursorRects", (0,0,550,140)WindowSetContentMinSize( _window, fn CGSizeMake( 250, 140 ) )subclass textlabel _label, @"Label:", (18,101,41,16)subclass textfield _txtFld,,, (65,99,348,21)ViewSetAutoresizingMask( _txtFld, NSViewMinYMargin + NSViewWidthSizable )subclass button _remFocusBtn,,, @"Remove Focus", (414,93,123,32)ViewSetAutoresizingMask( _remFocusBtn, NSViewMinXMargin + NSViewMinYMargin )subclass checkbox _checkbox,, NSControlStateValueOn, @"Checkbox", (231,61,87,18)ViewSetAutoresizingMask( _checkbox, NSViewMinXMargin + NSViewMaxXMargin + NSViewMinYMargin + NSViewMaxYMargin )subclass button _pushBtn,,, @"Push", (13,13,75,32)ViewSetAutoresizingMask( _pushBtn, NSViewMaxXMargin + NSViewMaxYMargin )end fnvoid local fn ResetCursorRects( tag as long )CursorRef cursorselect ( tag )case _txtFld      : cursor = fn CursorClosedHandcase _remFocusBtn : cursor = fn CursorCrosshaircase _label       : cursor = fn CursorOperationNotAllowedcase _checkbox    : cursor = fn CursorDragCopycase _pushBtn     : cursor = fn CursorPointingHandcase else         : cursor = fn CursorArrowend selectViewAddCursorRect( tag, fn ViewBounds(tag), cursor )end fnvoid local fn DoDialog( ev as long, tag as long )select ( ev )case _btnClickselect ( tag )case _remFocusBtn : WindowMakeFirstResponder( _window, 0 )end selectcase _viewResetCursorRectsselect ( tag )case _txtFld, _remFocusBtn, _label, _checkbox, _pushBtn : fn ResetCursorRects	( tag )end selectend selectend fnfn BuildWindowon dialog fn DoDialogHandleEvents