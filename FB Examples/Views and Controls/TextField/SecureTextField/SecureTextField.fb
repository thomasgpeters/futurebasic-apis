include "NSLog.incl"_window = 1begin enum 1_secureFldend enumvoid local fn BuildWindowwindow _window, @"SecureTextField", (0,0,290,62)// params: tag, enabled, text, rect, wndTagsecuretextfield _secureFld,, @"password", (20,20,250,22)ViewSetAutoresizingMask( _secureFld, NSViewMinYMargin + NSViewWidthSizable )// focus fieldWindowMakeFirstResponder( _window, _secureFld )end fnvoid local fn DoDialog( ev as long, tag as long, wnd as long )select ( ev )case _textFieldDidBeginEditingNSLog(@"text field %ld did begin editing in window %ld",tag,wnd)case _textFieldDidChangeNSLog(@"text field %ld did change in window %ld",tag,wnd)case _textFieldDidEndEditingNSLog(@"text field %ld did end editing in window %ld",tag,wnd)end selectend fneditmenu 1fn BuildWindowon dialog fn DoDialogHandleEvents