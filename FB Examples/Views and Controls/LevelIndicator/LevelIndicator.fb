/*  LevelIndicator  Note; some of these styles require macOS 10.10+*/include "NSLog.incl"_window = 1begin enum 1_ind1_ind2_ind3_ind4_ind5_ind6_ind7end enumvoid local fn BuildWindowwindow _window, @"Level Indicator", (0,0,340,310)// params: tag, enabled, value, rect, min, max, warning, critical, tickMarks, majorTickMarks, wndTaglevelindicator _ind1,, 2, (20,272,300,18), 0, 10LevelIndicatorSetEditable( _ind1, YES )levelindicator _ind2,, 6, (20,223,300,18), 0, 10, 6, 9LevelIndicatorSetEditable( _ind2, YES )levelindicator _ind3,, 8, (20,169,300,25), 0, 10, 6, 8, 11, 6LevelIndicatorSetEditable( _ind3, YES )levelindicator _ind4,, 3, (20,123,71,17), 0, 5,,, 6LevelIndicatorSetStyle( _ind4, NSLevelIndicatorStyleRating )LevelIndicatorSetEditable( _ind4, YES )levelindicator _ind5,, 2, (168,121,152,16), 0, 5,,, 6LevelIndicatorSetStyle( _ind5, NSLevelIndicatorStyleRelevancy )// macOS 10.10+LevelIndicatorSetEditable( _ind5, YES )levelindicator _ind6,, 2, (20,83,300,16), 0, 10, 6, 9LevelIndicatorSetStyle( _ind6, NSLevelIndicatorStyleContinuousCapacity )// macOS 10.10+LevelIndicatorSetEditable( _ind6, YES )levelindicator _ind7,, 4, (25,20,290,32), 0, 9LevelIndicatorSetStyle( _ind7, NSLevelIndicatorStyleRating )// macOS 10.10+LevelIndicatorSetEditable( _ind7, YES )ImageRef image = fn ImageNamed( NSImageNameAdvanced )ImageSetSize( image, fn CGSizeMake(32,32) )LevelIndicatorSetImage( _ind7, image )end fnvoid local fn DoDialog( ev as long, tag as long )select ( ev )case _btnClickselect (tag)case _ind5, _ind6NSLog(@"level indicator %ld value = %0.1f",tag,fn ControlDoubleValue(tag))case elseNSLog(@"level indicator %ld value = %ld",tag,fn ControlIntegerValue(tag))end selectend selectend fnfn BuildWindowon dialog fn DoDialogHandleEvents