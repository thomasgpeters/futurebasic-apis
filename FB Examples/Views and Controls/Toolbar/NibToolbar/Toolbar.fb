include "NSLog.incl"include resources "Cup.png"_window = 1_toolbar = 1void local fn BuildWindowwindow _window, @"Toolbar Demo", (0,0,550,400)// params: tag, nibNamenibtoolbar _toolbar, @"Toolbar"WindowSetToolbar( _window, _toolbar )end fnvoid local fn DoDialog( ev as long, tag as long )select ( ev )case _toolbarItemClickNSLog(@"toolbar item %ld clicked",tag)end selectend fnfn BuildWindowon dialog fn DoDialogHandleEvents