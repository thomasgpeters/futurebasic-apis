/*   Single-column outlineview*/include "Tlbx UTCoreTypes.incl"include "NSLog.incl"void local fn CreateOutlineViewDataCFMutableArrayRef   array = fn TableViewData(1)OutlineItemRef item = fn OutlineItemWithString( @"Charlie", NULL, YES )fn OutlineItemAddChildWithString( item, @"Echo", NULL, NO )fn OutlineItemAddChildWithString( item, @"Juliet", NULL, NO )fn OutlineItemAddChildWithString( item, @"Mike", NULL, NO )MutableArrayAddObject( array, item )item = fn OutlineItemWithString( @"Oscar", NULL, YES )fn OutlineItemAddChildWithString( item, @"Romeo", NULL, NO )fn OutlineItemAddChildWithString( item, @"Victor", NULL, NO )MutableArrayAddObject( array, item )item = fn OutlineItemWithString( @"Papa", NULL, YES )fn OutlineItemAddChildWithString( item, @"Zulu", NULL, NO )MutableArrayAddObject( array, item )TableViewReloadData(1)end fnvoid local fn AddFileToOutlineData( url as CFURLRef, destItem as OutlineItemRef )OutlineItemRef   parentItem//, itemBOOL isGroup = fn FileManagerIsDirectoryAtURL( url )CFStringRef string = fn URLLastPathComponent( url )//item = fn OutlineItemWithString( string, NULL, isGroup )if ( fn OutlineItemIsGroup( destItem ) )fn OutlineItemAddChildWithString( destItem, string, NULL, isGroup )elseparentItem = fn OutlineViewParentForItem( 1, destItem )fn OutlineItemAddChildWithString( parentItem, string, NULL, isGroup )end ifTableViewReloadData( 1 )end fnvoid local fn BuildWindownibwindow 1, @"Window"ViewRegisterForDraggedTypes( 1, @[OUTLINEVIEW_REORDER_PASTEBOARD_TYPE,kUTTypeFileURL] )fn CreateOutlineViewDataend fnfn BuildWindowHandleEvents