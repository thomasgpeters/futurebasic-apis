/*   Outlineview with images*/void local fn CreateOutlineViewDataCFMutableArrayRef   array = fn TableViewData(1)OutlineItemRef item = fn OutlineItemWithString( @"QuickLook", fn ImageNamed(NSImageNameQuickLookTemplate), YES )fn OutlineItemAddChildWithString( item, @"Bluetooth", fn ImageNamed(NSImageNameBluetoothTemplate), NO )fn OutlineItemAddChildWithString( item, @"IChatTheater", fn ImageNamed(NSImageNameIChatTheaterTemplate), NO )fn OutlineItemAddChildWithString( item, @"Slideshow", fn ImageNamed(NSImageNameSlideshowTemplate), NO )MutableArrayAddObject( array, item )item = fn OutlineItemWithString( @"ListView", fn ImageNamed(NSImageNameListViewTemplate), YES )fn OutlineItemAddChildWithString( item, @"ColumnView", fn ImageNamed(NSImageNameColumnViewTemplate), NO )fn OutlineItemAddChildWithString( item, @"FlowView", fn ImageNamed(NSImageNameFlowViewTemplate), NO )MutableArrayAddObject( array, item )item = fn OutlineItemWithString( @"RevealFreestanding", fn ImageNamed(NSImageNameRevealFreestandingTemplate), YES )fn OutlineItemAddChildWithString( item, @"InvalidDataFreestanding", fn ImageNamed(NSImageNameInvalidDataFreestandingTemplate), NO )MutableArrayAddObject( array, item )TableViewReloadData(1)end fnvoid local fn BuildWindownibwindow 1, @"Window"fn CreateOutlineViewDataend fnfn BuildWindowHandleEvents