include "NSLog.incl"_window = 1begin enum 1_tokenFldend enumvoid local fn BuildWindowwindow _window, @"TokenField", (0,0,400,61)// params: tag, enabled, text, rect, style, wndTagtokenfield _tokenFld,, @"Token", (20,20,360,21)ViewSetAutoresizingMask( _tokenFld, NSViewMinYMargin + NSViewWidthSizable )// focus a fieldWindowMakeFirstResponder( _window, _tokenFld )end fnvoid local fn DoDialog( ev as long, tag as long, wnd as long )select ( ev )case _textFieldDidBeginEditingNSLog(@"text field %ld did begin editing in window %ld",tag,wnd)case _textFieldDidChangeNSLog(@"text field %ld did change in window %ld",tag,wnd)case _textFieldDidEndEditingNSLog(@"text field %ld did end editing in window %ld",tag,wnd)end selectend fneditmenu 1fn BuildWindowon dialog fn DoDialogHandleEvents