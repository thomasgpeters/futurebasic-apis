/*  TableView Drag Drop 1  Drag re-order rows*/_window = 1begin enum output 1_tableViewend enumvoid local fn TableDataCreateCFMutableArrayRef tableDatatableData = fn TableViewData( _tableView )MutableArrayAddObject( tableData, @"Alpha" )MutableArrayAddObject( tableData, @"Bravo" )MutableArrayAddObject( tableData, @"Cocoa" )MutableArrayAddObject( tableData, @"Delta" )MutableArrayAddObject( tableData, @"Echo" )MutableArrayAddObject( tableData, @"FutureBasic" )MutableArrayAddObject( tableData, @"Golf" )MutableArrayAddObject( tableData, @"Hotel" )MutableArrayAddObject( tableData, @"India" )MutableArrayAddObject( tableData, @"Juliet" )TableViewReloadData( _tableView )end fnvoid local fn BuildWindownibwindow _window, @"Window"window _window, @"Reorder Rows"// TABLEVIEW_REORDER_PASTEBOARD_TYPE tells the drag mechanism to allow re-ordering of rowsViewRegisterForDraggedTypes( _tableView, @[TABLEVIEW_REORDER_PASTEBOARD_TYPE] )fn TableDataCreateend fnvoid local fn DoDialog( ev as long )select ( ev )case _windowWillClose : endend selectend fnfn BuildWindowon dialog fn DoDialogHandleEvents