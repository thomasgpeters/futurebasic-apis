/*   RowBackgroundColor*/include "NSLog.incl"void local fn CreateTableDataCFMutableArrayRef   tableDatatableData = fn TableViewData( 1 )MutableArrayAddObject( tableData, @"Alpha" )MutableArrayAddObject( tableData, @"Bravo" )MutableArrayAddObject( tableData, @"Charlie" )MutableArrayAddObject( tableData, @"Delta" )MutableArrayAddObject( tableData, @"Echo" )MutableArrayAddObject( tableData, @"Foxtrot" )MutableArrayAddObject( tableData, @"Golf" )MutableArrayAddObject( tableData, @"Hotel" )MutableArrayAddObject( tableData, @"India" )MutableArrayAddObject( tableData, @"Juliet" )MutableArrayAddObject( tableData, @"Kilo" )MutableArrayAddObject( tableData, @"Lima" )MutableArrayAddObject( tableData, @"Mike" )MutableArrayAddObject( tableData, @"November" )MutableArrayAddObject( tableData, @"Oscar" )MutableArrayAddObject( tableData, @"Papa" )MutableArrayAddObject( tableData, @"Quebec" )MutableArrayAddObject( tableData, @"Romeo" )MutableArrayAddObject( tableData, @"Sierra" )MutableArrayAddObject( tableData, @"Tango" )MutableArrayAddObject( tableData, @"Uniform" )MutableArrayAddObject( tableData, @"Victor" )MutableArrayAddObject( tableData, @"Whisky" )MutableArrayAddObject( tableData, @"Xray" )MutableArrayAddObject( tableData, @"Yankee" )MutableArrayAddObject( tableData, @"Zulu" )TableViewReloadData( 1 )end fnvoid local fn BuildWindownibwindow 1, @"Window"fn CreateTableDataend fnvoid local fn DoDialog( ev as long )TableRowViewRef   rowViewNSUInteger        rowColorRef          colselect ( ev )case _tableViewDidAddRowViewrowView = (TableRowViewRef)fn DialogEventViewcol = NULLrow = fn DialogEventLongselect ( row )case 2  : col = fn ColorRedcase 5  : col = fn ColorGreencase 11 : col = fn ColorYellowcase 16 : col = fn ColorMagentacase 22 : col = fn ColorOrangecase 23 : col = fn ColorCyan// case ....end selectif ( col ) then TableRowViewSetBackgroundColor( rowView, col )end selectend fnfn BuildWindowon dialog fn DoDialogHandleEvents