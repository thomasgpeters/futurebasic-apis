include resources "iChat Icons"void local fn TableDataAddRow( tableData as CFMutableArrayRef, string as CFStringRef, image as ImageRef )CFDictionaryRef   dictdict = @{@"Name":string,@"Image":image}MutableArrayAddObject( tableData, dict )end fnvoid local fn CreateTableDataCFMutableArrayRef   tableDataCFURLRef            fileURL//, dirURLCFArrayRef          filesImageRef            imageCFStringRef         nametableData = fn TableViewData( 1 )//dirURL = fn URLFileURLWithPath(@"/Library/Application Support/Apple/iChat Icons/Flags")// no longer here in macOS 10.14//files = fn FileManagerContentsOfDirectoryAtURL( dirURL, NULL, NSDirectoryEnumerationSkipsHiddenFiles )files = fn BundleURLsForResources( fn BundleMain, @"png", @"iChat Icons/Flags" )for fileURL in filesimage = fn ImageByReferencingURL( fileURL )name = fn StringByDeletingPathExtension( fn URLLastPathComponent( fileURL ) )fn TableDataAddRow( tableData, name, image )nextTableViewReloadData( 1 )end fnvoid local fn BuildWindownibwindow 1, @"Window"fn CreateTableDataend fnfn BuildWindowHandleEvents