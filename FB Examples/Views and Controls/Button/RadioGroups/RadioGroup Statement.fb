// Requires FB 7.0.26+include "NSLog.incl"_window = 1begin enum 1_group1_group2end enumvoid local fn BuildWindowwindow _window, @"Radio Groups"radiogroup _group1,,, @"High;Medium;Low",,,,, (20,320)radiogroup _group2,,, @"Top Left;Bottom Left;Top Right;Bottom Right",, 2,, (20,50)end fnvoid local fn DoDialog( ev as long, tag as long )select ( ev )case _btnClickNSLogClear : NSLog(@"Group: %ld, Button: %ld",tag,fn RadioGroupIndex(tag))end selectend fnfn BuildWindowon dialog fn DoDialogHandleEvents