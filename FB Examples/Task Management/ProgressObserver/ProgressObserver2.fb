/*  ProgressObserver2  macOS 14.0+*/_window = 1_progBar = 1_unitCount = 100void local fn DoItwindow _window, @"ProgressIndicatorSetObservedProgress"progressbar _progBar,, (20,20,510,20)ProgressRef progress = fn ProgressWithTotalUnitCount( _unitCount )ProgressIndicatorSetObservedProgress( _progBar, progress )block TimerRef t = timerbegin , 0.1, YESUInt64 count = fn ProgressCompletedUnitCount( progress ) + 1ProgressSetCompletedUnitCount( progress, count )if ( count > _unitCount ) then TimerInvalidate( t ) : print @"done"timerendend fnfn DoItHandleEvents