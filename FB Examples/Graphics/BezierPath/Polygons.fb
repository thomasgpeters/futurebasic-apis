void local fn BuildWindowwindow 1, @"Polygons", (0,0,380,260)WindowSubclassContentView(1)end fnvoid local fn DrawPolygonsBezierPathRef path = fn BezierPathWithPolygonInRectRotate( fn CGRectMake(20,125,100,100), 3, 0, 0, 180 )BezierPathStrokeFill( path, 3, fn ColorBlack, fn ColorYellow )path = fn BezierPathWithPolygonInRectRotate( fn CGRectMake(140,140,100,100), 5, 4, 0, 180 )BezierPathStrokeFill( path, 3, fn ColorBlack, fn ColorSystemPink )path = fn BezierPathWithPolygonInRectRotate( fn CGRectMake(260,140,100,100), 5, 4, 20, 180 )BezierPathStrokeFill( path, 3, fn ColorBlack, fn ColorGreen )path = fn BezierPathWithPolygonInRect( fn CGRectMake(20,20,100,100), 6, 3, 0 )BezierPathStrokeFill( path, 3, fn ColorBlack, fn ColorSystemIndigo )path = fn BezierPathWithPolygonInRect( fn CGRectMake(140,20,100,100), 7, 3, 0 )BezierPathStrokeFill( path, 3, fn ColorBlack, fn ColorSystemMint )path = fn BezierPathWithPolygonInRect( fn CGRectMake(260,20,100,100), 8, 3, 0 )BezierPathStrokeFill( path, 3, fn ColorBlack, fn ColorSystemOrange )	end fnvoid local fn DoDialog ( ev as long )select ( ev )case _viewDrawRect : fn DrawPolygonsend selectend fnfn BuildWindowon dialog fn DoDialoghandleevents