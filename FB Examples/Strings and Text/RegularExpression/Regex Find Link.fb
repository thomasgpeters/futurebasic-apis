/*  Regex Find Link  Bernie Wylde 20181224*/include "NSLog.incl"CFStringRef             string, urlStringDataDetectorRef         ddCFArrayRef              matchesTextCheckingResultRef   matchCFRange                 rangestring = @"This URL: http://www.brilorsoftware.com/fb is FutureBasic's home page."dd = fn DataDetectorWithTypes( NSTextCheckingTypeLink, NULL )matches = fn RegularExpressionMatches( dd, string, 0, fn CFRangeMake(0,fn StringLength(string)) )for match in matchesrange = fn TextCheckingResultRange( match )urlString = fn StringSubstringWithRange( string, range )NSLog(@"%@",urlString)nextHandleEvents