include "NSLog.incl"void local fn DoItCFNumberRef   num1 = @12, num2 = @24, num3 = @36, num4 = @48CFStringRef   s1 = @"Ford", s2 = @"Toyota", s3 = @"BMW", s4 = @"Mercedes", s5 = @"Porsche"long          val1 = 5, val2 = 8, val3 = 12, val4 = 17// array of number variablesCFArrayRef array = @[num1,num2,num3,num4]NSLog(@"%@",array)// array of number literalsarray = @[@9,@8,@7,@6,@5,@4,@3,@2,@1]NSLog(@"%@",array)// array of long variables - note parentheses are required around the variable namearray = @[@(val1),@(val2),@(val3),@(val4)]NSLog(@"%@",array)// array of string literalsarray = @[@"Alpha",@"Bravo",@"Charlie",@"Delta",@"Echo"]NSLog(@"%@",array)// array of string variablesarray = @[s1,s2,s3,s4,s5]NSLog(@"%@",array)CFStringRef car = fn ArrayObjectAtIndex( array, 3 )NSLog(@"%@",car)end fnfn DoItHandleEvents