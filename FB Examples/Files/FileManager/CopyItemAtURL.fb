/*   CopyItemAtURL*/include "NSLog.incl"void local fn DoItCFURLRef     itemURL, dstDirURL, copyURLCFStringRef  itemNameitemURL = openpanel 1, @"Choose item to copy",, @"Choose"if ( itemURL )OpenPanelSetCanChooseFiles( 1, NO )OpenPanelSetCanChooseDirectories( 1, YES )dstDirURL = openpanel 1, @"Choose destination directory",, @"Choose"if ( dstDirURL )itemName = fn URLLastPathComponent( itemURL )copyURL = fn URLByAppendingPathComponent( dstDirURL, itemName )NSLog(@"Item copied: %d",fn FileManagerCopyItemAtURL( itemURL, copyURL ))end ifend ifend fnfn DoItHandleEvents