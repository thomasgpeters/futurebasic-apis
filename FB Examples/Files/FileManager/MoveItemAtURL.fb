/*   MoveItemAtURL*/include "NSLog.incl"void local fn DoItCFURLRef    fromURL, dstDirURL, toURLCFStringRef itemNamefromURL = openpanel 1, @"Choose item to move",, @"Choose"if ( fromURL )OpenPanelSetCanChooseFiles( 1, NO )OpenPanelSetCanChooseDirectories( 1, YES )dstDirURL = openpanel 1, @"Choose destination directory",, @"Choose"if ( dstDirURL )itemName = fn URLLastPathComponent( fromURL )toURL = fn URLByAppendingPathComponent( dstDirURL, itemName )if ( fn FileManagerMoveItemAtURL( fromURL, toURL ) )NSLog(@"Item moved")elseNSLog(@"Item was not moved")end ifend ifend ifend fnfn DoItHandleEvents