include "NSLog.incl"void local fn LoadNibWindow// params: wndTag, nibName, wndIdentifiernibwindow 1, @"Window", @"Window"end fnvoid local fn DoDialoglong ev, id, wndev = dialog(0)id = dialog(ev)wnd = dialog(-1)select ( ev )case _btnClickNSLog(@"button %ld clicked in window %ld", id, wnd)textfield 1,,@"New Text in textfield"case _textFieldDidChangeNSLog(@"field %ld did change in window %ld", id, wnd)case _comboBoxSelectionDidChangeNSLog(@"combo box %ld selection did change to index %ld", id, fn ComboBoxIndexOfSelectedItem(id))end selectend fnfn LoadNibWindowon dialog fn DoDialogHandleEvents