/*  DateFormatter*/include "NSLog.incl"void local fn Example1CFDateRef dt = fn DateInitDateFormatterRef df = fn DateFormatterInitDateFormatterSetDateStyle( df, NSDateFormatterShortStyle )CFStringRef string = fn DateFormatterStringFromDate( df, dt )NSLog(@"Example 1: %@",string)end fnvoid local fn Example2CFDateRef dt = fn DateInitDateFormatterRef df = fn DateFormatterInitDateFormatterSetDateFormat( df, @"MM/dd/yyy hh:mma" )CFStringRef string = fn DateFormatterStringFromDate( df, dt )NSLog(@"Example 2: %@",string)end fnvoid local fn Example3CFDateRef dt = fn DateInitDateFormatterRef df = fn DateFormatterInitDateFormatterSetDateFormat( df, @"EEEE MMMM/d/YYYY hh:mma" )CFStringRef string = fn DateFormatterStringFromDate( df, dt )NSLog(@"Example 3: %@",string)end fnvoid local fn Example4CFDateRef dt = fn DateInitDateFormatterRef df = fn DateFormatterInitDateFormatterSetDateFormat( df, @"h:mm a, zzz" )CFStringRef string = fn DateFormatterStringFromDate( df, dt )NSLog(@"Example 4: %@",string)end fn// date from stringvoid local fn Example5CFStringRef string = @"20180526"DateFormatterRef df = fn DateFormatterInitDateFormatterSetDateFormat( df, @"yyyyMMdd" )CFDateRef dt = fn DateFormatterDateFromString( df, string )DateFormatterSetDateFormat( df, @"EEEE MMMM d, YYYY" )string = fn DateFormatterStringFromDate( df, dt )NSLog(@"Example 5: %@",string)end fn// localevoid local fn Example6CFDateRef dt = fn DateInitDateFormatterRef df = fn DateFormatterInitDateFormatterSetDateFormat( df, @"EEEE MMMM d, YYYY" )DateFormatterSetLocale( df, fn LocaleWithIdentifier( @"ja_JP" ) )CFStringRef string = fn DateFormatterStringFromDate( df, dt )NSLog(@"Example 6: %@",string)end fnfn Example1fn Example2fn Example3fn Example4fn Example5fn Example6HandleEvents