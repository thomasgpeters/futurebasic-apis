/*  A little demo to obtain information about all your connected printers.  Ken Shmidheiser 20180619*/include "NSLog.incl"void local fn DoItCFStringRef   namePrinterRef    printerCGSize        paperSizeNSLog( @"Printer information:\n" )CFArrayRef availablePrinters = fn PrinterNamesfor name in availablePrintersNSLog( @"Printer name:\t%@", name )printer = fn PrinterWithName( name )NSLog( @"Printer type:\t%@", fn PrinterType( printer) )NSLog( @"Printer PostScript Language Level (not a PostScript printer if 0):  %d", fn PrinterLanguageLevel( printer) )paperSize = fn PrinterPageSizeForPaper( printer, @"Letter" )NSLog( @"Letter paper size:\t%1.f x %1.f", paperSize.width, paperSize.height )paperSize = fn PrinterPageSizeForPaper( printer, @"Legal" )NSLog( @"Legal paper size:\t%1.f x %1.f", paperSize.width, paperSize.height )paperSize = fn PrinterPageSizeForPaper( printer, @"A4" )NSLog( @"A4 paper size:\t\t%1.f x %1.f", paperSize.width, paperSize.height )NSLog( @"Printer description:\n%@\n\n", fn PrinterDeviceDescription( printer) )nextend fnfn DoItHandleEvents