/*  PersonNameComponents  macOS 10.11+  Note: The order in which components appear are determined by Contacts.app preferences.*/include "NSLog.incl"void local fn DoItPersonNameComponentsRef comps = fn PersonNameComponentsInitPersonNameComponentsSetNamePrefix( comps, @"Mr." )PersonNameComponentsSetGivenName( comps, @"Robert" )PersonNameComponentsSetMiddleName( comps, @"Allen" )PersonNameComponentsSetFamilyName( comps, @"Zimmerman" )CFStringRef string = fn PersonNameComponentsFormatterLocalizedStringFromComponents( comps, NSPersonNameComponentsFormatterStyleLong, 0 )NSLog(@"%@",string)end fnfn DoItHandleEvents