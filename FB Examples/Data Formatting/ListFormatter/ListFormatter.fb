/*  ListFormatter  macOS 10.15+*/include "NSLog.incl"void local fn DoIt// stringsCFArrayRef names = @[@"Tom",@"Dick",@"Harry"]CFStringRef string = fn ListFormatterLocalizedStringByJoiningStrings( names )NSLog(@"%@",string)// numbersNumberFormatterRef nf = fn NumberFormatterInitNumberFormatterSetNumberStyle( nf, NSNumberFormatterSpellOutStyle )ListFormatterRef lf = fn ListFormatterInitListFormatterSetItemFormatter( lf, nf )string = fn ListFormatterStringFromItems( lf, @[@10,@20,@30] )NSLog(@"%@",string)nf = fn NumberFormatterInitNumberFormatterSetNumberStyle( nf, NSNumberFormatterOrdinalStyle + NSNumberFormatterSpellOutStyle )lf = fn ListFormatterInitListFormatterSetItemFormatter( lf, nf )string = fn ListFormatterStringFromItems( lf, @[@1,@2,@3,@4,@5] )NSLog(@"%@",string)end fnfn DoItHandleEvents