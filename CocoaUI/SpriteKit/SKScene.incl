r( SKSceneRef ref, SKNodeRef listener )//toolbox fn SKSceneAudioEngine( SKSceneRef ref ) = AVAudioEngineRef// Converting between coordinate systemstoolbox fn SKSceneConvertPointFromView( SKSceneRef ref, CGPoint pt ) = CGPointtoolbox fn SKSceneConvertPointToView( SKSceneRef ref, CGPoint pt ) = CGPoint