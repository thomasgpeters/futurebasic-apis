/*  DistributedNotificationCenter.incl  Bernie Wylde 20180427  Framework: Foundation  Inherits from: NotificationCenter/Object  NSDistributedNotificationCenter - https://developer.apple.com/documentation/foundation/nsdistributednotificationcenter*/// options#if ndef _DEFINEDINCOCOA#define NSDistributedNotificationOptions as NSUInteger#endifsystem NSDistributedNotificationOptions NSDistributedNotificationDeliverImmediatelysystem NSDistributedNotificationOptions NSDistributedNotificationPostToAllSessions  system NSDistributedNotificationOptions NSNotificationDeliverImmediatelysystem NSDistributedNotificationOptions NSNotificationPostToAllSessions// type#if ndef _DEFINEDINCOCOA#define NSDistributedNotificationCenterType as CFStringRef#endif// behavior#if ndef _DEFINEDINCOCOA#define NSNotificationSuspensionBehavior as NSUInteger#endifsystem NSNotificationSuspensionBehavior NSNotificationSuspensionBehaviorDropsystem NSNotificationSuspensionBehavior NSNotificationSuspensionBehaviorCoalescesystem NSNotificationSuspensionBehavior NSNotificationSuspensionBehaviorHoldsystem NSNotificationSuspensionBehavior NSNotificationSuspensionBehaviorDeliverImmediately// Classtoolbox fn DistributedNotificationCenterClass = ClassRef// Default centertoolbox fn DistributedNotificationCenterDefaultCenter = DistributedNotificationCenterReftoolbox fn DistributedNotificationCenterForType( NSDistributedNotificationCenterType type ) = DistributedNotificationCenterRef// Observers/*  DistributedNotificationCenterAddObserver & DistributedNotificationCenterAddObserverWithSuspensionBehavior    callback example:    void local fn MyCallback( ref as NotificationRef )    end fn*/toolbox DistributedNotificationCenterAddObserver( ptr callback, CFStringRef name, CFTypeRef obj )toolbox DistributedNotificationCenterAddObserverWithSuspensionBehavior( ptr callback, CFStringRef name, CFTypeRef obj, NSNotificationSuspensionBehavior behavior )toolbox DistributedNotificationCenterRemoveObserver( ptr callback, CFStringRef name )// Notificationstoolbox DistributedNotificationCenterPostNotificationName( CFStringRef name, CFTypeRef obj, CFDictionaryRef userInfo, NSUInteger options )// Suspendtoolbox fn DistributedNotificationCenterSuspended( DistributedNotificationCenterRef ref ) = BOOLtoolbox DistributedNotificationCenterSetSuspended( DistributedNotificationCenterRef ref, BOOL flag )