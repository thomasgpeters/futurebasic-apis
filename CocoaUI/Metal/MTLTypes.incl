/*  MTLTypes  Bernie Wylde 20241204  Framework: Metal*/// mtl origin#if ndef _DEFINEDINCOCOAbegin record MTLOriginNSUInteger xNSUInteger yNSUInteger zend record#endiftoolbox fn MTLOriginMake( NSUInteger x, NSUInteger y, NSUInteger z ) = MTLOrigin// mtl size#if ndef _DEFINEDINCOCOAbegin record MTLSizeNSUInteger widthNSUInteger heightNSUInteger depthend record#endiftoolbox fn MTLSizeMake( NSUInteger width, NSUInteger height, NSUInteger depth ) = MTLSize// mtl region#if ndef _DEFINEDINCOCOAbegin record MTLRegionMTLOrigin originMTLSize   sizeend record#endiftoolbox fn MTLRegionMake1D( NSUInteger x, NSUInteger width ) = MTLRegiontoolbox fn MTLRegionMake2D( NSUInteger x, NSUInteger y, NSUInteger width, NSUInteger height ) = MTLRegiontoolbox fn MTLRegionMake3D( NSUInteger x, NSUInteger y, NSUInteger z, NSUInteger width, NSUInteger height, NSUInteger depth ) = MTLRegion// mtl sample position#if ndef _DEFINEDINCOCOAbegin record MTLSamplePositionfloat xfloat yend record#endiftoolbox fn MTLSamplePositionMake( float x, float y ) = MTLSamplePosition// mtl coordinate 2D#if ndef _DEFINEDINCOCOA#define MTLCoordinate2D as MTLSamplePosition#endiftoolbox fn MTLCoordinate2DMake( float x, float y ) = MTLCoordinate2D// mtl resource ID#if _MACOSMAXALLOWED >= 130000#if ndef _DEFINEDINCOCOAbegin record MTLResourceID // macOS 13.0+UInt64 _implend record#endif // _DEFINEDINCOCOA#endif // 130000