/*  PointerFunctions.incl  Bernie Wylde 20180507  Framework: Foundation  Inherits from: Object  NSPointerFunctions - https://developer.apple.com/documentation/foundation/nspointerfunctions?language=objc*/// function options#if ndef _DEFINEDINCOCOA#define NSPointerFunctionsOptions as NSUInteger#endifsystem NSPointerFunctionsOptions NSPointerFunctionsStrongMemorysystem NSPointerFunctionsOptions NSPointerFunctionsZeroingWeakMemorysystem NSPointerFunctionsOptions NSPointerFunctionsOpaqueMemorysystem NSPointerFunctionsOptions NSPointerFunctionsMallocMemorysystem NSPointerFunctionsOptions NSPointerFunctionsMachVirtualMemorysystem NSPointerFunctionsOptions NSPointerFunctionsWeakMemorysystem NSPointerFunctionsOptions NSPointerFunctionsObjectPersonalitysystem NSPointerFunctionsOptions NSPointerFunctionsOpaquePersonalitysystem NSPointerFunctionsOptions NSPointerFunctionsObjectPointerPersonalitysystem NSPointerFunctionsOptions NSPointerFunctionsCStringPersonalitysystem NSPointerFunctionsOptions NSPointerFunctionsStructPersonalitysystem NSPointerFunctionsOptions NSPointerFunctionsIntegerPersonalitysystem NSPointerFunctionsOptions NSPointerFunctionsCopyIn// Classtoolbox fn PointerFunctionsClass = ClassRef// Createtoolbox fn PointerFunctionsWithOptions( NSPointerFunctionsOptions options ) = PointerFunctionsRef// Personality functions/*  Hash function example:    local fn MyPointerHashFunction( item as ptr, size as NSUInteger )    end fn*/toolbox fn PointerFunctionsHashFunction( PointerFunctionsRef ref ) = ptrtoolbox PointerFunctionsSetHashFunction( PointerFunctionsRef ref, ptr fnAddress )/*  IsEqual function example:    local fn MyPointerIsEqualFunction( item1 as ptr, item2 as ptr, size as NSUInteger ) as BOOL    end fn = item1 == item2*/toolbox fn PointerFunctionsIsEqualFunction( PointerFunctionsRef ref ) = ptrtoolbox PointerFunctionsSetIsEqualFunction( PointerFunctionsRef ref, ptr fnAddress )/*  Size function example:    local fn MyPointerSizeFunction( item as ptr ) as NSUInteger    end fn = size*/toolbox fn PointerFunctionsSizeFunction( PointerFunctionsRef ref ) = ptrtoolbox PointerFunctionsSetSizeFunction( PointerFunctionsRef ref, ptr fnAddress )/*  Description function example:    local fn MyPointerDescriptionFunction( item as ptr ) as CFStringRef    end fn = fn StringWithFormat( @"%d", item )*/toolbox fn PointerFunctionsDescriptionFunction( PointerFunctionsRef ref ) = ptrtoolbox PointerFunctionsSetDescriptionFunction( PointerFunctionsRef ref, ptr fnAddress )// Memory configuration/*  Acquire function example:    local fn MyPointerAcquireFunction( src as ptr, size as NSUInteger, shouldCopy as BOOL ) as ptr      ptr newPtr = malloc(sizeof(long))      *newPtr = src// ??    end fn = newPtr*/toolbox fn PointerFunctionsAcquireFunction( PointerFunctionsRef ref ) = ptrtoolbox PointerFunctionsSetAcquireFunction( PointerFunctionsRef ref, ptr fnAddress )/*  Relinquish function example:    local fn MyPointerRelinquishFunction( item as ptr, size as NSUInteger )      free(item)    end fn*/toolbox fn PointerFunctionsRelinquishFunction( PointerFunctionsRef ref ) = ptrtoolbox PointerFunctionsSetRelinquishFunction( PointerFunctionsRef ref, ptr fnAddress )