/*  Touch.incl  Bernie Wylde 20181107  Framework: AppKit  Inherits from: Object  NSTouch - https://developer.apple.com/documentation/appkit/nstouch?language=objc*/// touch types#if ndef _DEFINEDINCOCOA#define NSTouchType as NSInteger#endifsystem NSTouchType NSTouchTypeDirectsystem NSTouchType NSTouchTypeIndirect// touch type mask#if ndef _DEFINEDINCOCOA#define NSTouchTypeMask as NSInteger#endifsystem NSTouchTypeMask NSTouchTypeMaskDirectsystem NSTouchTypeMask NSTouchTypeMaskIndirect// phase#if ndef _DEFINEDINCOCOA#define NSTouchPhase as NSUInteger#endifsystem NSTouchPhase NSTouchPhaseBegansystem NSTouchPhase NSTouchPhaseMovedsystem NSTouchPhase NSTouchPhaseStationarysystem NSTouchPhase NSTouchPhaseEndedsystem NSTouchPhase NSTouchPhaseCancelledsystem NSTouchPhase NSTouchPhaseTouching // = NSTouchPhaseBegan + NSTouchPhaseMoved + NSTouchPhaseStationarysystem NSTouchPhase NSTouchPhaseAny      // = NSUIntegerMax// Classtoolbox fn TouchClass = ClassRef// Typetoolbox fn TouchType( TouchRef ref ) = NSTouchType// Properties of this touchtoolbox fn TouchIdentity( TouchRef ref ) = CFTypeReftoolbox fn TouchPhase( TouchRef ref ) = NSTouchPhasetoolbox fn TouchNormalizedPosition( TouchRef ref ) = CGPointtoolbox fn TouchIsResting( TouchRef ref ) = BOOL// Properties of touch devicetoolbox fn TouchDevice( TouchRef ref ) = CFTypeReftoolbox fn TouchDeviceSize( TouchRef ref ) = CGSize// Touch locationtoolbox fn TouchLocationInView( TouchRef ref, NSInteger tag ) = CGPoint         toolbox fn TouchPreviousLocationInView( TouchRef ref, NSInteger tag ) = CGPoint