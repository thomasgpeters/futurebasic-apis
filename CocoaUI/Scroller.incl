/*  Scroller.incl  Bernie Wylde 20180604  Framework: AppKit  Inherits from: Control/View/Responder/Object  NSScroller - https://developer.apple.com/documentation/appkit/nsscroller?language=objc*/// scroller style#if ndef _DEFINEDINCOCOA#define NSScrollerStyle as NSInteger#endifsystem NSScrollerStyle NSScrollerStyleLegacysystem NSScrollerStyle NSScrollerStyleOverlay// scroller part#if ndef _DEFINEDINCOCOA#define NSScrollerPart as NSUInteger#endifsystem NSScrollerPart NSScrollerKnobsystem NSScrollerPart NSScrollerKnobSlotsystem NSScrollerPart NSScrollerDecrementPagesystem NSScrollerPart NSScrollerIncrementPagesystem NSScrollerPart NSScrollerNoPart// usable scroller parts#if ndef _DEFINEDINCOCOA#define NSUsableScrollerParts as NSUInteger#endifsystem NSUsableScrollerParts NSNoScrollerPartssystem NSUsableScrollerParts NSOnlyScrollerArrows// deprecated: 10.14system NSUsableScrollerParts NSAllScrollerParts// knob style#if ndef _DEFINEDINCOCOA#define NSScrollerKnobStyle as NSInteger#endifsystem NSScrollerKnobStyle NSScrollerKnobStyleDefaultsystem NSScrollerKnobStyle NSScrollerKnobStyleDarksystem NSScrollerKnobStyle NSScrollerKnobStyleLight// notificationssystem CFStringRef NSPreferredScrollerStyleDidChangeNotification// Classtoolbox fn ScrollerClass = ClassRef// Scroller sizetoolbox fn ScrollerWidth( NSControlSize size, NSScrollerStyle style ) = CGFloattoolbox fn ScrollerControlSize( ScrollerRef ref ) = NSControlSize// Knob positiontoolbox fn ScrollerKnobProportion( ScrollerRef ref ) = CGFloattoolbox ScrollerSetKnobProportion( ScrollerRef ref, CGFloat proportion )// Calculate layouttoolbox fn ScrollerRectForPart( ScrollerRef ref, NSScrollerPart part ) = CGRecttoolbox fn ScrollerTestPart( ScrollerRef ref, CGPoint pt ) = NSScrollerParttoolbox fn ScrollerUsableParts( ScrollerRef ref ) = NSUsableScrollerParts// Draw scroller partstoolbox ScrollerDrawKnobSlotInRect( ScrollerRef ref, CGRect r, BOOL highlight )// Event handlingtoolbox fn ScrollerHitPart( ScrollerRef ref ) = NSScrollerPart// Presentation styletoolbox fn ScrollerPreferredScrollerStyle = NSScrollerStyletoolbox fn ScrollerScrollerStyle( ScrollerRef ref ) = NSScrollerStyletoolbox fn ScrollerKnobStyle( ScrollerRef ref ) = NSScrollerKnobStyle// Type propertiestoolbox fn ScrollerIsCompatibleWithOverlayScrollers = BOOL