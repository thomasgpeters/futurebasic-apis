/*  CAGradientLayer.incl - https://developer.apple.com/documentation/quartzcore/cagradientlayer?language=objc  Bernie Wylde 20180507  Framework: QuartzCore  Inherits from: CALayer*/include "CocoaUI/CoreAnimation/CALayer.incl"// typessystem CFStringRef kCAGradientLayerAxial// Inittoolbox fn CAGradientLayerInit = CAGradientLayerRef// autoreleased// Styletoolbox fn CAGradientLayerColors( CAGradientLayerRef layer ) = CFArrayRef     // array of ColorRefstoolbox CAGradientLayerSetColors( CAGradientLayerRef layer, CFArrayRef cols )     // array of ColorRefstoolbox fn CAGradientLayerLocations( CAGradientLayerRef layer ) = CFArrayRef// array of CFNumberRefs (values between 0 and 1)toolbox CAGradientLayerSetLocations( CAGradientLayerRef layer, CFArrayRef locations )// array of CFNumberRefs - can be NULLtoolbox fn CAGradientEndPoint( CAGradientLayerRef layer ) = CGPointtoolbox CAGradientLayerSetEndPoint( CAGradientLayerRef layer, CGPoint pt )toolbox fn CAGradientStartPoint( CAGradientLayerRef layer ) = CGPointtoolbox CAGradientLayerSetStartPoint( CAGradientLayerRef layer, CGPoint pt )toolbox fn CAGradientType( CAGradientLayerRef layer ) = CFStringReftoolbox CAGradientLayerSetType( CAGradientLayerRef layer, CFStringRef type )// Conveniencetoolbox fn CAGradientLayerWithStartEndColors( CGRect r, ColorRef startColor, CGPoint startPt, ColorRef endColor, CGPoint endPt ) = CAGradientLayerRef