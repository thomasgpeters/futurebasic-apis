/*  Range.incl  Bernie Wylde 20181120  Framework: Foundation  NSRange - https://developer.apple.com/documentation/foundation/nsrange?language=objc*//*  MakeRange  Creates a new CFRange from the specified values.*/toolbox fn MakeRange( NSUInteger location, NSUInteger length ) = CFRangetoolbox fn RangeMake( NSUInteger location, NSUInteger length ) = CFRange// same as MakeRange/*  MaxRange  Returns the sum of the location and length of the range.*/toolbox fn MaxRange( CFRange range ) = NSUIntegertoolbox fn RangeMax( CFRange range ) = NSUInteger// same as MaxRange/*  LocationInRange  Returns a BOOL value that indicates whether a specified position is in a given range.*/toolbox fn LocationInRange( CFRange range, NSUInteger location ) = BOOLtoolbox fn RangeLocation( CFRange range, NSUInteger location ) = BOOL// same as LocationInRange/*  EqualRanges  Returns a BOOL value that indicates whether two given ranges are equal.*/toolbox fn EqualRanges( CFRange range1, CFRange range2 ) = BOOLtoolbox fn RangeEqual( CFRange range1, CFRange range2 ) = BOOL// same as EqualRanges/*  UnionRange  Returns the union of the specified ranges.*/toolbox fn UnionRange( CFRange range1, CFRange range2 ) = CFRangetoolbox fn RangeUnion( CFRange range1, CFRange range2 ) = CFRange// same as UnionRange/*  IntersectionRange  Returns the intersection of the specified ranges.*/toolbox fn IntersectionRange( CFRange range1, CFRange range2 ) = CFRangetoolbox fn RangeIntersection( CFRange range1, CFRange range2 ) = CFRange// same as IntersectionRange/*  RangeFromString  Returns a range from a textual representation.*/toolbox fn RangeFromString( CFStringRef string ) = CFRange/*  StringFromRange  Returns a string representation of a range.*/toolbox fn StringFromRange( CFRange range ) = CFStringReftoolbox fn RangeToString( CFRange range ) = CFStringRef// same as StringFromRange/*  Value additions*/toolbox fn ValueWithRange( CFRange 